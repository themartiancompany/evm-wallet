__evm_wallet_list() {
  local \
    cur \
    prev \
    opts
  COMPREPLY=()
  cur="${COMP_WORDS[COMP_CWORD]}"
  prev="${COMP_WORDS[COMP_CWORD-1]}"
  opts="-R -S -r -u -l -o -t -C -w -p -s -n -k -h -v -c"
  if [[ ${cur} == * ]] ; then
      COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
      return 0
  fi
}

complete -F __evm_wallet_list evm-wallet

